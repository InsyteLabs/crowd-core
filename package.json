{
  "name": "crowd-core",
  "version": "1.0.0",
  "description": "Voting application for conferences and organizations",
  "main": "dist/index.js",
  "scripts": {
    "start": "nodemon dist/index.js",
    "test": "jest",
    "tsc": "tsc",
    "tsc-watch": "tsc-watch --onSuccess 'npm run copy-sql'",
    "copy-sql": "cp -r `find src/db/queries/ -type d` dist/db/queries",
    "build": "npm run tsc && npm run copy-sql",
    "seed": "node dist/db-seeder.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/InsyteLabs/crowd-core.git"
  },
  "author": "Bryce Jech <bryce@brycejech.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/InsyteLabs/crowd-core/issues"
  },
  "homepage": "https://github.com/InsyteLabs/crowd-core#readme",
  "dependencies": {
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "pg": "^7.12.1",
    "uuid": "^3.3.3",
    "ws": "^7.1.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/express": "^4.17.1",
    "@types/jsonwebtoken": "^8.3.5",
    "@types/pg": "^7.11.2",
    "@types/uuid": "^3.4.5",
    "@types/ws": "^6.0.3",
    "db-migrate-pg": "^1.0.0",
    "jest": "^24.9.0",
    "source-map-support": "^0.5.13",
    "tsc-watch": "^4.0.0",
    "typescript": "^3.6.4"
  }
}
