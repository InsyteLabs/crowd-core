{
  "name": "crowd-core",
  "version": "1.0.0",
  "description": "Voting application for conferences and organizations",
  "main": "build/index.js",
  "scripts": {
    "start": "node build/index.js",
    "test": "jest",
    "tsc": "tsc",
    "tsc-watch": "tsc-watch --onSuccess 'npm run copy-sql'",
    "copy-sql": "cp -r `find src/db/queries/ -type d` build/db/queries",
    "build": "npm run tsc && npm run copy-sql"
  },
