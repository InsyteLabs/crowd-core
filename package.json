    "url": "https://github.com/InsyteLabs/crowd-core/issues"
  },
  "homepage": "https://github.com/InsyteLabs/crowd-core#readme",
  "dependencies": {
    "@types/pg": "^7.11.1",
    "bcrypt": "^3.0.6",
    "express": "^4.17.1",
    "pg": "^7.12.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.1",
    "db-migrate-pg": "^1.0.0",
    "jest": "^24.9.0",
    "tsc-watch": "^4.0.0",
    "typescript": "^3.6.3"
  }
